	.slick-dots {
	    @include rtl-left(50%);
	    @include translateX(-50%);
	    position: absolute;
	    bottom: 33px;
		&>li {
			color: $theme-color-default;
			button {
				background-color: transparent;
				cursor: pointer;
				@include display(inline-block);
				padding: 0;
				position: relative;
				outline:none;
				font-size: 12px;
				&:before {
					content: "";
					@include transition(all 0.2s ease-in);
					position: absolute;
					@include border-radius(50%);
					@include rtl-left(50%);
					top: 50%;
					@include translate(-50%,-50%);
					@include opacity(1);
					z-index: -1;
				}
			}
		}
		.slick-active {
			color: $theme-color-default;
		}
	}
	.slick-arrow {
		@include square(50px);
		background: rgba(255,255,255,.23);
		@include border-radius(100%);
		position: absolute;
		@include opacity(0);
		visibility: hidden;
		top: 50%;
		@include translateY(-50%);
		z-index: 1;
		cursor: pointer;
		.zmdi {
			font-size: 3rem;
			color: $color-bg;
			@include square(45px);
			line-height: 45px;
		}
		i {
