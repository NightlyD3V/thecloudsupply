    }
  }
}

.canvas-menu {
  background: #333;
  color: $theme-color-heading;
  position: fixed;
  z-index: 9999;
  top: 0;
  @include rtl-left(auto);
  @include rtl-right(auto);
  width: 250px;
  height: 100%;
  @include transition($transition-drawer);
  overflow: hidden;
  -webkit-overflow-scrolling: touch;
  &.drawer-left {
    @include rtl-right(-250px);
  }
  #canvas-main-menu {
    top: 45px;
    background: $color-bg;
    .mm-navbar .mm-title {
      color: $theme-color-default !important;
      font-size: 1.4rem;
    }
    .mm-panel {
      visibility: visible;
      @include opacity(1);
    }
  }
  .canvas-header-box {
    /* @include gradient-horizontal($color_gradient_1, $color_gradient_3); */
    background-color: $theme-color-default;
    min-height: 45px;
    padding: 0 20px;
    font-size: 1.4rem;
    font-weight: 700;
    color: $color-bg;
    .close-box {
      cursor: pointer;
    }
  }
  .site-nav {
    > li {
      @include rtl-text-align-left();
      @include display(block);
      &:before {
        @include display(none);
      }
      > .mm-next {
        &:before {
          @include display(none);
        }
        &:after {
          @include square(6px);
          border-top: 1.5px solid;
          @include rtl-border-left(1.5px solid);
        }
      }
      &:not(.mm-divider)::after {
        @include rtl-left(0);
        border-bottom: 1px dashed #e1e1e1;
      }
      > a:not(.mm-next) {
        font-family: $font-family-primary;
        color: #5b5b5b;
        font-size: 12px;
        @include rtl-padding(20px, 10px, 20px, 20px !important);
      }
      > .site-nav__link {        
        > i {@include display(none);}
      }
    }
  }
  .site-nav__dropdown {
    li:after {
      @include display(none);
    }
    .site-nav__link {
      padding-top: 10px;
      padding-bottom: 10px;
      font-size: 13px;
      font-family: $font-family-primary;
    }
    &.megaMenu .novMenuLinks {
      a {
        padding: 10px 0;
        @include display(inline-block);
      }
    }
  }
  .novMenuBanner {
    margin-bottom: 20px;
  }
  .site-nav__childlist-item {
    .site-nav__child-link--parent {
      text-transform: uppercase;
      font-size: 1.4rem;
      color: $color-heading;
      font-weight: 700;
    }
  }
}
body {
  @include transition($transition-drawer);
  position: relative;
  @include rtl-left(0);
  @include rtl-right(0);
  &:not(.template-index) {
    .nov-footer {
      margin-top: 75px;
    }
    #_desktop_vertical_menu {
      @include display(none);
    }
  }
  &.canvasmenu-left {
    overflow: hidden;
    @include rtl-right(250px);
    @include rtl-left(-250px);
    .canvas-overlay {
      @include opacity(1);
      visibility: visible;
    }
    .canvas-menu {
      &.drawer-left {
        @include rtl-right(0);
      }
    }
  }
  .canvas-overlay, .sidebar-overlay {
    position: absolute;
    @include rtl-left(0);
    @include rtl-right(0);
    top: 0;
    bottom: 0;
    z-index: 90;
    background: rgba(0, 0, 0, 0.5);
    @include opacity(0);
    visibility: hidden;
    @include transition(all 0.3s linear);
    &.act {
      @include opacity(1);
      visibility: visible;
    }
  }
}

