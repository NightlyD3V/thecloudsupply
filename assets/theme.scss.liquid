/*============================================================================
	Animation Classes and Keyframes
==============================================================================*/
.is-transitioning {
	// sass-lint:disable no-important
	@include display(block !important);
	visibility: visible !important;
}

@include keyframes(spin) {
	0% {
		@include transform(rotate(0deg));
	}

	100% {
		@include transform(rotate(360deg));
	}
}

.drawer {
	// sass-lint:disable no-misspelled-properties
	@include display(none);
	position: absolute;
	overflow: hidden;
	-webkit-overflow-scrolling: touch;
	z-index: $z-index-drawer;
	background-color: $color-bg;
	transition: $transition-drawer;

	input[type="text"],
	textarea {
		background-color: $color-bg;
		color: $color-body-text;
	}
}

.js-drawer-open {
	overflow: hidden;
}

.drawer--top {
	width: 100%;

	.js-drawer-open-top & {
		@include transform(translateY(100%));
		@include display(block);
	}
}

.drawer-page-content::after {
	visibility: hidden;
	@include opacity(0);
	content: '';
	@include display(block);
	position: fixed;
	top: 0;
	@include rtl-left(0);
	@include square(100%);
	background-color: $color-drawer-background;
	z-index: $z-index-drawer - 1;
	transition: $transition-drawer;

	.js-drawer-open & {
		visibility: visible;
		@include opacity(1);
	}
}
.page-container {
	.mt-10 {
		@media (min-width: 768px) {
			@include flexbox();
		}
	}
}

.no-gutters {
	.ProductRelated {
		.grid--view-items {
			.owl-stage-outer {
				.product_scale_down {
					@include rtl-border-left(none !important);
				}
			}
		}
	}
}
.drawer__title,
.drawer__close {
	@include display(table-cell);
	vertical-align: middle;
}

.drawer__close-button {
	background: none;
	border: 0 none;
	position: relative;
	@include rtl-right(-15px);
	height: 100%;
	width: 60px;
	padding: 0 20px;
	color: inherit;
	font-size: em(18);

	&:active,
	&:focus {
		background-color: darken($color-drawer-background, 5%);
	}
}

/*============================================================================
	Slick slider overrides
==============================================================================*/
$slick-dot-size: 12px;
$slick-dot-size-small: 21px;

.slick-dotted.slick-slider {
	margin-bottom: 0;
}

/*================ Slick dots and prev/next ================*/
.slick-slider .slick-dots {
	margin: 0;
	width: auto;

	li {
		// sass-lint:disable SelectorDepth
		vertical-align: middle;
		@include square($slick-dot-size-small);

		button {
			position: relative;
			@include square($slick-dot-size-small);
		}

		button::before {
			text-indent: -9999px;
			background-color: transparent;
			@include border-radius(100%);
			background-color: red;
			@include square($slick-dot-size-small);
		}

		&.slick-active button::before {
			@include opacity(1);
		}

		button:active::before {
			@include opacity(0.7);
		}
	}
}


$color-blankstate: rgba($color-body-text, 0.35);
$color-blankstate-border: rgba($color-body-text, 0.2);
$color-blankstate-background: rgba($color-body-text, 0.1);

.placeholder-svg {
	@include display(block);
	fill: $color-blankstate;
	background-color: $color-blankstate-background;
	@include square(100%);
	max-width: 100%;
	max-height: 100%;
	border: 1px solid $color-blankstate-border;
}

.placeholder-noblocks {
	padding: 40px;
	text-align: center;
}

// Mimic a background image by wrapping the placeholder svg with this class
.placeholder-background {
	position: absolute;
	top: 0;
	@include rtl-right(0);
	bottom: 0;
	@include rtl-left(0);

	.icon {
		border: 0;
	}
}

// Custom styles for some blank state images
.gallery-image__content .placeholder-svg {
	position: absolute;
	top: 0;
	@include rtl-left(0);
}


