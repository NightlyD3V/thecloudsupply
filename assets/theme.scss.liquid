		position: relative;
		.loadingImage {
			position: absolute;
			top: 50%;
			@include translateY(-50%);
			z-index: 1000;
			overflow: hidden;
			background: transparent;
			width: 100%;
			height: 60px;
			@include opacity(1);
			.loader {
				position: absolute;
				top: 10px;
				@include rtl-left(50%);
				@include translateX(-50%);
