/*===========================================
  Vinova SliderShow
====================================================*/
$slideshow-height-small: 475px;
$slideshow-height-medium: 650px;
$slideshow-height-large: 775px;
$slideshow-loader: #fff;
$z-index-slideshow-image: 1;
$z-index-slideshow-video: 2;
$z-index-slideshow-text: 3;
$z-index-slideshow-controls: 4;
$color-slideshow-close-bg: #fff;
$color-slideshow-close-text: #000;

.slideshow-wrapper {
  position: relative;
}
.section-slideshow {
  overflow: hidden;
}

.slideshow {
  overflow: hidden;
  margin-bottom: 0;
  // Make sure slides fill full height

  .slick-prev,
  .slick-next {
    top: 50%;
    @include translateY(-50%);
    @include square(45px);
    line-height: 45px;
    margin-top: -21px;
    z-index: 1;
    i {
      font-size: 3rem;
      color: #ccc;
    }
    &:hover {
      i {color: $color-heading;}
    }
  }
  .slick-list {
    overflow: inherit;
  }

  .slick-prev {
    @include rtl-left(22px);
  }

  .slick-next {
    @include rtl-right(22px);
  }
}

figure {
  margin: 0;
}

.video-is-playing .slick-dots {
  // sass-lint:disable no-important
  @include display(none !important);
}

// Pause button (focusable by keyboard only)
.slideshow__pause:focus {
  clip: auto;
  @include square(auto);
  margin: 0;
  color: $color-btn-primary-text;
  background-color: $color-btn-primary;
  padding: $gutter-site / 2;
  z-index: $z-index-skip-to-content;
  transition: none;

  .video-is-playing & {
    @include display(none);
  }
}

.slideshow__pause-stop {
  @include display(block);

  .is-paused & {
    @include display(none);
  }
}

.slideshow__pause-play {
  @include display(none);

  .is-paused & {
    @include display(block);
  }
}

/*================ General slide styles ================*/
.slideshow__slide {
  position: relative;
  overflow: hidden;
}

.slideshow__link {
  @include display(block);
  position: absolute;
  top: 0;
  @include rtl-left(0);
  @include rtl-right(0);
  bottom: 0;

  &:active,
  &:focus {
    @include opacity(1);
  }
}

.slideshow__overlay {
  
}

/*================ Slide images ================*/
.slideshow__image {
  transition: $transition-image-slideshow;
  position: absolute;
  top: 0;
  @include rtl-left(0);
  @include opacity(0);
  @include square(100%);
  background-repeat: no-repeat;
  background-size: cover;
  background-position: top center;
  z-index: $z-index-slideshow-image;

  .slick-initialized &,
  .no-js & {
    @include opacity(1);
  }

  .slideshow__slide--background-video & {
    @include opacity(0);
  }

  .no-autoplay & {
    @include opacity(1);
  }
}

// z-index stacking issues in oldIE
.ie9 {
  .slideshow__slide {
    // sass-lint:disable no-important
    z-index: 1 !important;
  }

  .slick-dots {
    z-index: 2;
  }
}

/*================ Slide text ================*/
.slideshow__text-wrap {
  height: 100%;

  .slideshow__link & {
    cursor: inherit;
  }

  .slideshow__slide--has-background-video & {
    padding-top: $gutter-site * 3;
  }

  .video-is-playing & {
    @include display(none);
  }

  .slideshow__slide.video-is-paused & {
    @include display(none);
  }
}

.slideshow__text-content {
  text-align: center;
  position: absolute;
  width: 100%;
  top: 50%;
  @include transform(translateY(-40%));
  @include opacity(0);
  transition: $transition-text-slideshow;
  transition-delay: 0.3s;
  z-index: $z-index-slideshow-text;

  .slick-active &,
  .no-js & {
    @include transform(translateY(-50%));
    @include opacity(1);
  }

  &::after {
    content: '';
    @include animation(spin 0.65s infinite linear);
    @include opacity(1);
    transition: all 1s cubic-bezier(0.29, 0.63, 0.44, 1);
    bottom: -$gutter-site;
    @include rtl-left(50%);
  }

  .slick-initialized &,
  .no-js & {
    &::after {
      @include opacity(0);
      visibility: hidden;
      content: none;
    }
  }
}

.slideshow__title {
  color: $color-overlay-title-text;
}

.slideshow__subtitle {
  @include display(block);
  color: $color-overlay-title-text;
}

/*================ Video styles ================*/
.slideshow__slide--has-background-video::after {
  content: '';
  position: absolute;
  top: 0;
  @include rtl-left(0);
  @include rtl-right(0);
  bottom: 0;
  z-index: $z-index-slideshow-video;
}

.slideshow__video {
  @include display(none);
  position: absolute;
  @include rtl-left(0);
  top: 0;
  z-index: $z-index-slideshow-video;
}

// Background video
.slideshow__video--background {
  position: relative;
  visibility: hidden;
  @include opacity(0);
  transition: all 0.2s ease-in;

  .autoplay &.video-is-loaded {
    @include display(block);
    visibility: visible;
    @include opacity(1);
  }
}

// Place invisible element over video to prevent clicks
.slideshow__slide--background-video::after {
  content: '';
  @include display(block);
  position: absolute;
  top: 0;
  @include rtl-left(0);
  @include rtl-right(0);
  bottom: 0;
  z-index: $z-index-slideshow-video;
}

.slideshow__video--chrome {
  @include display(none);
  @include opacity(0);
  visibility: none;
  @include square(100%);
  transition: all 0.2s ease-in;

  // YouTube video will not load if iframe is display: none
  .ie9 & {
    @include display(block);
  }

  .slideshow__slide.video-is-playing &,
  .slideshow__slide.video-is-paused & {
    @include display(block);
    visibility: visible;
    @include opacity(1);
  }
}

/*================ Video control buttons ================*/
.slideshow__video-control {
  @include display(none);
  visibility: hidden;
  @include opacity(0);
  position: absolute;
  padding: 5px;
  z-index: $z-index-slideshow-controls;
  transition: all 0.1s ease-out;

  &:hover,
  &:focus {
    @include opacity(0.7);
  }
}

// Video loader shown when initializing
.video-loader {
  @include animation(spin 0.65s infinite linear);
  transition: all 0.1s ease-out 0.5s;
  z-index: $z-index-slideshow-controls;
  top: 50%;
  @include rtl-left(50%);

  .ie9 &,
  .video-is-loaded &,
  .video-is-playing &,
  .video-is-paused &,
  .autoplay &,
  .no-autoplay & {
    content: none;
    @include display(none);
  }

  .video-is-loading &,
  .autoplay .video-is-loading &,
  .no-autoplay .video-is-loading & {
    @include display(block);
    visibility: visible;
    @include opacity(1);
  }
}

.slideshow__video-control--play-wrapper {
  height: 30px;

  @include media-query($medium-up) {
    height: 45px;
  }
}

.slideshow__video-control--play {
  @include opacity(0);
  color: $color-overlay-title-text;
  position: relative;
  margin: 0 auto;

  .slideshow__video--background {
    top: 50%;
    @include transform(translateY(-50%));
  }

  .video-is-loaded & {
    @include display(block);
    visibility: visible;
    @include opacity(1);
  }

  .video-is-loading &,
  .video-is-playing &,
  .slideshow__slide.video-is-paused & {
    @include display(none);
    visibility: hidden;
    @include opacity(0);
  }

  .icon {
    width: 42px;
    height: 100%;

    @include media-query($medium-up) {
      width: 65px;
    }
  }
}

.slideshow__video-control--close {
  top: 10px;
  @include rtl-right(10px);
  background-color: #343434;
  color: $color-bg;
  .video-is-playing &,
  .slideshow__slide.video-is-paused & {
    @include display(block);
    visibility: visible;
    @include opacity(1);
  }
  .icon {
    @include display(block);
    @include square(20px);
  }
}

