				}
				&.l {
					font-size: 3.5rem;
				}
				&.xl {
					font-size: 4rem;
				}
			}
			.caption-2 {
				font-size: 1.5rem;
			}
			.caption-3 br {
				@include display(none);
			}
		}
	}
	@media (max-width: 532px) {
		.content-caption {
			.content-1 {
				@include rtl-left(25%);
			}
			.content-2 {
			    @include rtl-left(52%);
			}
			.content-3 {
				@include rtl-margin-right(10px);
			}
		}
	}
	@media (max-width: 495px) {
		.content-caption {
			.content-1 {
				@include rtl-left(25%);
				@include rtl-margin-right(25px);
			}
			.content-2 {
			    @include rtl-left(52%);
			}
			.content-3 {
				@include rtl-margin-right(10px);
			}
		}
	}
	@media (max-width: 447px) {
		.content-caption {
			.content-1 {
				@include rtl-margin-right(0);
				.caption-3 {
					@include display(none);
				}
			}
			.content-3 {
				.caption-1 {
					line-height: 1;
				}
			}
		}
	}
	@media (max-width: 427px) {
		.content-caption .caption-1 {
			&.s {
				font-size: 1.8rem !important;
			}
			&.m {
				font-size: 3rem;
			}
			&.l {
				font-size: 3.5rem;
			}
			&.xl {
				font-size: 4rem;
			}
		}
		.caption-3 {
			font-size: 1.4rem !important;

		}
	}
	@media (max-width: 390px) {
		@include rtl-left(30%);
	}
	@media (max-width: 320px) {
		.content-caption {
			.content-1 {
				@include rtl-left(10%);
			}
			.content-2 {
				.caption-2, .caption-3 {
					@include display(none);
				}
