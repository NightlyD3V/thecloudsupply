.list-filter {
	.filter-item {
		&:not(:last-child) {
			margin-bottom: 25px;
		}
		li:hover {
			.custom-checkbox {
				border: 1px solid $theme-color-default;
			}
			.filter-option-value, .filter-option-amount {
				color: $theme-color-default;
			}
		}
		.filter-item_title {
			color: $color-heading-second;
			font-size: 12px;
			text-transform: uppercase;
			font-family: $font-family-primary;
			font-weight: 600;
			padding-bottom: 6px;
			border-bottom: 1px dotted #b8b8b8;
			margin-bottom: 25px;
		}
		.custom-checkbox {
			border: 1px solid #dbdbdb;
			@include border-radius(2px);
			@include square(16px);
			position: relative;
			@include display(inline-block);
			vertical-align: middle;
			@include rtl-margin-right(8px);
			&:before {
				content: "";
				font-family: "FontAwesome";
				font-size: 1.4rem;
				color: $color-bg;
				position: absolute;
				top: -4px;
			}
			@media (min-width: 1200px) {
				&:hover {
					border: 1px solid $theme-color-default;
				}
			}
		}
		.filter-item_content {
