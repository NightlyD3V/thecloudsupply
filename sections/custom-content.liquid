                            {% if block.settings.title != blank %}
                              <div class="title_block {{ block.settings.title_alignment }}{% if block.settings.title_alignment == 'text-left' and block.settings.tab_align == 'right' %} alignment{% endif %}{% if block.settings.tab_title_1 == blank and block.settings.tab_title_2 == blank and block.settings.tab_title_3 == blank %} no_tab{% endif %}">
                                {{ block.settings.title }}
                                <div class="sub_title">{{ block.settings.sub_title }}</div>
                              </div>
                            {% endif %}
                            {%- assign collection = collections[block.settings.collection] -%}

                            {%- assign img_size = settings.img_size_large -%}
                            {% if block.settings.grid_number == '5' %}
                              {%- assign grid_item_width = 'col-lg-cus-5' -%}
                            {% else %}
                              {%- assign grid_item_width = 12 | divided_by: block.settings.grid_number | prepend: 'col-lg-' -%}
                            {% endif %}

                            {% if block.settings.grid_number_tablet == '5' %}
