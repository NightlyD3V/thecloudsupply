// Swatch =================================================================================
.swatch {
	// margin-bottom: 25px;
	@include align-items(center);
	@include display(inline-block);
	.swatch-element {
		@include rtl-float-left();
		position: relative;
		&:not(:last-child) {
			@include rtl-margin-right(10px);
			// margin-top: 5px;
		}
		.crossed-out {
			@include display(none);
		}
		&:not(.color) {
			label {
				@include square(42px);
				line-height: 4.2rem;
				min-width: 42px;
				@include transition(all 0.2s linear);
				border: 1px solid #d7d7d7;
			}
			input:checked + label, label:hover {
				border-color: $theme_color_default;
				color: $theme_color_default;
			}
		}
		&.color {
			label {
				@include square(26px);
				min-width: 26px;
				@include transition(all 0.2s linear);
			}
			input:checked + label, label:hover {
				border: 2px solid $color-bg;
				@include box-shadow(0 0 0 1px #d8d8d8);
			}
		}
		&.white {
			label {
				border: 1px solid #d7d7d7;
			}
		}
	}
	input {
		@include display(none);
	}
	.header {
		font-size: 12px;
		margin-top: 5px;
		font-weight: bold;
		color: $color-heading-second;
		@include display(inline-block);
	    float: left;
	    @include rtl-margin-right(10px);
	}
	label {
		@include border-radius(4px);
		margin: 0;
		text-align: center;
		@include rtl-float-left();
		font-size: 12px;
		text-transform: uppercase;
		white-space: nowrap;
		cursor: pointer;
		padding: 0 5px;
		font-family: $font-family-default;
	}
	input:checked + label, label:hover {
		border-color: #343434;
		border-width: 2px;
		line-height: 2.8rem;
	}
	.crossed-out {
		position: absolute;
		@include square(100%);
		@include rtl-left(0);
		top: 0;
	}
}

