/* Map Section =======================================================================================================*/
.map-section {
	margin-top: 5px;
	position: relative;
	width: 100%;
	overflow: hidden;
	@include flexbox();
	@include align-items(center);
	@include flex-wrap(wrap);
	@include flex-direction(row);
	@media (min-width: 768px) {
		min-height: 400px;
	}
	@media (max-width: 767px) {
		padding-top: 0px;
	}
}

.map-section--load-error {
	height: auto;
}

.map-section__wrapper {
	height: 100%;
	flex-shrink: 0;
	flex-grow: 1;
	@include flex-basis(100%);
	@include flexbox();
	@include flex-wrap(wrap);
	@include flex-direction(row);
}

.map-section__overlay {
	position: absolute;
	top: 0;
	@include rtl-right(0);
	bottom: 0;
	@include rtl-left(0);
	@include opacity(0);
	z-index: 2;
}

.map-section__error {
	position: relative;
	z-index: 3;

	 @media (min-width: 768px) {
		position: absolute;
		margin: 0 2rem;
		top: 50%;
		@include translateY(-50%);
	}
}

.map-section__content-wrapper {
	position: relative;
	text-align: center;
	height: 100%;
	@include flexbox();
	@include flex-basis(100%);
	flex-grow: 0;

	 @media (min-width: 768px) {
		@include flex-basis(50%);
	}

	 @media (min-width: 1200px) {
		@include flex-basis(33%);
	}
}

.map-section__content {
	position: relative;
	@include display(inline-block);
	background-color: $color-bg;
	padding: 50px 0;
	width: 100%;
	text-align: center;
	z-index: 3;
	@include flexbox();
	@include align-items(center);
	@include flex-wrap(wrap);
	@include align-content(center);

	// Make sure every children is on one line
	& > * {
		width: 100%;
	}

	 @media (min-width: 768px) {
		background-color: $color-bg;
		margin: 60px 0;
		min-height: 300px;

		.ie9 & {
			top: 10%;
		}
	}

	.map-section--load-error & {
		position: static;
		@include translateY(0);
	}
	.map_section__directions-btn {
		padding: 10px 20px;
		&:hover {
			color: $color-bg !important;
			background: $theme-color-default;
		}
	}
}

.map-section__link {
	@include display(block);
	position: absolute;
	top: 0;
	@include rtl-left(50%);
	max-width: none;
	@include square(100%);
	z-index: 2;
	@include translateX(-50%);
}

// Optically center map in visible area with
// extended height/widths and negative margins
.map-section__container {
	max-width: none;
	width: 100%;
	height: 55vh;
	@include rtl-left(0);
	z-index: 3;

 	@media (min-width: 768px) {
		position: absolute;
		height: 100%;
		top: 0;
		// map is centered on resize, larger widths allow pin to be off-center
		width: 130%;
	}
}

.map_section__directions-btn {
	[class^="icon"] {
		height: 1em;
	}

	* {
		vertical-align: middle;
	}
}

.map-section__background-wrapper {
	overflow: hidden;
	position: relative;
	@include flex-basis(100%);

 @media (min-width: 768px) {
		position: absolute;
		@include rtl-left(0);
		top: 0;
		@include square(100%);
	}

	.ie9 & {
		width: 100%;
		height: 500px;
	}

	.map-section--onboarding & {
		min-height: 55vh;
	}
}

.map-section__image {
	height: 100%;
	position:relative;
	top: 0;
	@include rtl-left(0);
	width: 100%;
	background-size: cover;
	background-position: center;

 @media (min-width: 768px) {
		position: absolute;
	}

	// Only show the background image if map fails to load
	.map-section--display-map & {
		@include display(none !important);
	}

 .map-section--load-error & {
		@include display(block !important);
	}
}

// Hide Google maps UI
.gm-style-cc,
.gm-style-cc + div {
	visibility: hidden;
}

