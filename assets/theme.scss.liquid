		    @include justify-content(center);
		    line-height: 50px;
		    color: $color-bg;
		    font-size: 2rem;
		}
		&.arrow-prev {
			@include rtl-left(19%);
		}
		&.arrow-next {
			@include rtl-right(-2%);
		}
	}
	@media (min-width: 1200px) {
		&:hover {
			.slick-arrow {
				@include opacity(1);
				visibility: inherit;
				@include transition(all 0.35s ease);
				&:hover {
					&.arrow-next, &.arrow-prev {
						background: $theme-color-default;
						color: $color-bg;
					}
				}
				&.arrow-prev {
					@include rtl-left(25%);
				}
				&.arrow-next {
					@include rtl-right(1%);
				}
			}
		}
