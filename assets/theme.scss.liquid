// Collection link
.nov-group-collection-tabs {
	.title_block {
		font-size: 2.6rem;
		margin-bottom: 5px;
	}
	.nav-tab {
		margin-bottom: 45px;
		li {
			position: relative;
			&:not(:last-child) {
				@include rtl-margin-right(20px);
				&:before {
					content: "";
					position: absolute;
					width: 1px;
					height: 70%;
					top: 50%;
					@include translateY(-50%);
					@include rtl-right(-10px);
					background: #909090;
				}
			}
			a {
				color: #909090;
				font-size: 11px;
				font-weight: 500;
				&:hover {
					text-decoration: none;
					color: $theme-color-primary;
				}
			}
		}
	}
	.nov-slick-carousel {
		margin: 0 -15px;
		.item {
			padding: 0 15px;
		}
		.item-product {
			background: none;
			&:before {
				@include display(none);
			}
			.thumbnail-container {
				background: #f5f5f5;
			}
			.product__info {
				padding: 15px 0 0;
				.product__title a {
					font-size: 13px;
					color: $theme-color-heading;
				}
				.product__review {
					margin-bottom: 5px;
				}
				.spr-badge-caption {
					@include display(none);
				}
			}
		}
		.slick-arrow {
			top: 35%;
			@include translateY(-50%);
			z-index: 99;
			@include opacity(0);
			@include transition(all 0.3s linear);
			&.arrow-prev {
				@include rtl-left(17px);
			}
			&.arrow-next {
				@include rtl-right(17px);
			}
		}
		&:hover {
			.slick-arrow {
				@include opacity(1);
			}
		}
	}
	@media (max-width: 1599px) {
		.nov-slick-carousel {
			margin: 0 -5px;
			.item {
				padding: 0 5px;
			}
		}
	}
	@media (max-width: 1199px) {
		.nav-tab {
			overflow-x: scroll;
			white-space: nowrap;
		}
	}
	@media (max-width: 991px) {
		.nav-tab {
			@include justify-content(center);
		}
	}
	@media (max-width: 767px) {
		.nav-tab {
			@include justify-content(inherit);
		}
	}
}

