                                    data-items_lg_tablet="{{ block.settings.grid_number_lg_tablet }}"
                                    data-items_tablet="{{ block.settings.grid_number_tablet }}"
                                    data-items_mobile="{{ block.settings.grid_number_mobile }}"
                                    data-row="{{ block.settings.row_number }}"
                                    data-loop="true"
                                    data-dots="{{ block.settings.show_dots }}"
                                    data-nav="{{ block.settings.show_nav }}"
                                    data-autoplay="false"
                                    data-autoplayTimeout="6000">
