// vertical menuTitle
.btn_close_sidebar {
	position: absolute;
	@include rtl-left(50%);
	top: 30px;
	font-size: 2.4rem;
	color: $font-family-primary;
	@include transition(all 0.3s linear);
	cursor: pointer;
	&:hover {
		@include rotate(180deg);
	}
}
.sidebarmenu_title {
	font-size: 2rem;
	font-family: $font-family-primary;
	color: $color-bg;
	@include rtl-margin-right(35px);
	cursor: pointer;
	@include rtl-padding(8px, 54px, 8px, 15px);
	@include border-radius(5px);
	background: $theme-color-primary;
	i {
		font-size: 1.6rem;
	}
	&:hover {
		background: $theme-color-default;
	}
}
.sidebarmenu_content {
	width: 284px;
	position: fixed;
	z-index: 9999;
	top: 0;
	@include rtl-left(-360px);      
	@include box-shadow(5px 0 12px rgba(0,0,0,.25));
	@include transition(all 0.3s ease 0.15s);
	a:hover {
		text-decoration: none;
	}
	.sidebarmenu_list_content {
		@include rtl-padding(115px, 30px, 40px, 30px);
		background: $color-bg;
		overflow-y: auto;
		height: 100vh;
		&::-webkit-scrollbar-track{
			background-color: #eaeaea;
			@include border-radius(20px);
		}
		&::-webkit-scrollbar {
			width: 3px;
			background-color: #eaeaea;
			@include border-radius(20px);
		}
		&::-webkit-scrollbar-thumb {
			@include border-radius(20px);
			background-color: $theme-color-primary;
		}
		.site-nav--has-dropdown:before {
			@include display(none);
		}
		> li {
			position: relative;
			> a {
				font-size: 1.6rem;
				color: $theme-color-default;
				font-weight: 700;
				text-transform: uppercase;
				padding: 10px 0;
				@include display(block);
				&:hover {
					color: $theme-color-primary;
				}
			}
			.show-sub {
				@include square(15px);
				line-height: 1.5rem;
				text-align: center;
				background: #cfcfcf;
				@include border-radius(3px);
				@include display(inline-block);
				color: $color-btn-primary-text;
				font-size: 1.5rem;
				position: absolute;
				@include rtl-right(0);
				top: 17px;
				cursor: pointer;
				i {
					vertical-align: bottom;
				}
				.zmdi-minus {
					@include display(none);
				}
				&.active {
					background: $theme-color-default;
					.zmdi-minus {
						@include display(block);
					}
					.zmdi-plus {
						@include display(none);
					}
				}
			}
			.site-nav__dropdown {
				position: static;
				box-shadow: none;
				visibility: visible;
				@include opacity(1);
				border: none;
				@include display(none);
				@include transition(none);
				ul {
					list-style-type: none;
				}
				li {
					border: none;
				}
				.site-nav__link {
					font-size: 11px;
					font-weight: 500;
					padding: 10px 0;
					@include display(block);
					color: #909090;
					text-transform: uppercase;
					&:hover {
						color: $theme-color-primary;
						background: none;
					}
				}
			}
			&.site-nav--has-dropdown:first-child {
				> .site-nav__dropdown {
					@include display(block);
				}
			}
		}
	}
	&.active {
		@include rtl-left(0);
		.btn_close_sidebar {
			@include opacity(1);
			visibility: visible;
		}
	}
}

