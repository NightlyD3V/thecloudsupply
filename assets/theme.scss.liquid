/* Cart page ===============================================================================================================================*/
.cart {
	th,
	td {
		border: 0;
	}

	td {
		padding: 10px 0;
	}

	th {
		font-family: $font-family-primary;
		padding: 30px 0 50px;
		font-size: 2rem;
		color: $theme-color-heading;
		font-weight: 500;
	}
	.cart__meta {
		@include rtl-padding-right(15px);
		.list-view-item__title {
			font-family: $font-family-primary;			
			font-size: 1.6rem;
			a {
				color: $theme-color-default;
			}
		}
		.cart__remove {
			color: $theme-color-second;
			&:hover {
				color: #ff684a !important;
			}
		}
		.cart__update-wrapper {
			span{
				color: $theme-color-default;
				&:hover {
					color: $theme-color-default;
				}
			}
		}
	}
	.cart__price-wrapper {
		font-family: $font-family-primary;
		color: $color-sale-text;
		font-size: 1.4rem;
		font-weight: bold;
	}
}

.cart__meta-text {
	padding: 5px 0;
	font-size: 1.4rem;
	font-style: italic;
}

.cart__qty-label {
	@include visually-hidden();
}

.cart__qty-input {
	text-align: center;
	width: 60px;
	@include rtl-padding-left(5px);
	@include rtl-padding-right(5px);

	@media(max-width: 480px) {
		padding-top: 2px;
		padding-bottom: 2px;
	}
}

.cart__edit {
	margin-top: 10px;
}

.cart__edit-text--cancel {
	.cart__edit--active & {
		@include display(none);
	}
}

.cart__edit-text--edit {
	@include display(none);

	.cart__edit--active & {
		@include display(block);
	}
}

.cart__edit-text--cancel,
.cart__edit-text--edit {
	pointer-events: none;
}

.cart__row {
	p {
		margin-bottom: 0;

		+ p {
			margin-top: 10px;
		}
	}
}
.cart__update-wrapper {
	.js-qty {@include display(inline-block);}
}

.cart__subtotal {
	@include rtl-padding-left(15px);
	@media(max-width: 768px) {
		@include rtl-padding-left(30px);
		min-width: 150px;
		@include display(inline-block);
	}
}

.cart__savings {
	padding-top: 18px;
}

.cart__savings-amount {
	@include rtl-padding-left(15px);

	@media(max-width: 768px) {
		@include rtl-padding-left(30px);
		min-width: 150px;
		@include display(inline-block);
	}
}

.cart__footer {
	padding-top: 60px;
	padding-bottom: 10px;
	.cart-note__input {
		min-height: 150px;
	}
	.cart__subtotal-title {
		font-family: $font-family-primary;
		font-size: 1.6rem;
		color: $color-heading-second;
		font-weight: 700;
	}
	.cart__subtotal {
		font-size: 2.2rem;
		font-weight: bold;
		@include rtl-padding-left(55px);
		font-family: $font-family-primary;
		color: $theme-color-default;
	}
}

.cart__update--large {
	@include rtl-margin-right(10px);
}

.cart__continue--large {
	@include rtl-margin-right(10px);
	// line-height: 1.2;
}

.cart__shipping {
	font-style: italic;
	font-size: 12px;
	padding: 18px 0 20px;
}

.cart-note__label,
.cart-note__input {
	@include display(block);
	@media (max-width: 480px) {
		margin: 0 auto;
	}
}

.cart-note__label {
	margin-bottom: 15px;
}

.cart-note__input {
	min-height: 50px;
	width: 100%;
	@media (max-width: 480px) {
		margin-bottom: 40px;
	}
}

.cart__image {
	max-height: 95px;
}

.cart__image-wrapper a {
	@include display(block);
}
.cart__remove {
	font-size: 1.9rem;
	background: none;
	color: #ff684a;
	padding: 0;
	border: none;
	&:hover {
		border: none;
		background: none;
		color: $color-heading;
	}
}
.total, .cart__price-wrapper {
	.hidden {
		@include display(none);
	}
}

@media (max-width: 767px) {
	.cart__qty {
		text-align: center;
	}
	.cart-flex {
		@include display(flex);
		@include align-items(center);
		@include flex-wrap(wrap);
		.total .hidden, .cart__price-wrapper .hidden {
			@include display(block);
			color: $theme_color_default;
		}
		.cart__qty, .total, .cart__price-wrapper {
			@include display(flex);
			@include align-items(center);
			@include justify-content(space-between);
			width: 100%;
		}
		.cart__price-wrapper, .cart__update-wrapper {
			font-weight: 700;
		}
		.cart__image-wrapper {
			@include flex(0 0 30%);
			@include rtl-padding(20px, 15px, 0, 0);
			width: 100%;
		}
		.cart__meta {
			@include flex(0 0 70%);
			@include rtl-padding-right(0);
			.list-view-item__title {
				span {
					@include display(none);
				}
			}
			p {
				@include rtl-margin-right(0 !important);
			}
		}
		.cart__price-wrapper {      
			@include flex(1 1 100%);
			padding: 20px 0;
		}
		.cart__update-wrapper {  
			width: 100%;   
			.cart__qty {
				.cart__qty-label {
					position: inherit !important;
					overflow: auto;
					clip: auto;
					@include square(auto);
					margin: 0;
					@include display(inline-block);
					vertical-align: middle;
					font-size: 1em;
					@include rtl-margin-right(5px);
				}
			}
		}
		.total {
			padding: 20px 0;
		}
	}
}

@media (max-width: 480px) {
	.cart {
		table {
			@include display(block);
			width: 100%;
		}

		thead {
			@include display(none);
		}

		tr,
		tbody {
			width: 100%;
		}

		tbody {
			@include display(block);
		}

		.cart__update-wrapper {
			padding-top: 0;
			padding-bottom: 10px;
		}
	}

	.cart__update--show {
		td {
			padding-bottom: 10px;
		}

		.cart__update-wrapper {

		}
	}

	.cart-flex {
		//@include flex(1 1 100%);
	}

	.cart-flex-item {
		@include display(block);
		min-width: 0;
		//@include flex(1 1 100%);
	}

	.cart__meta {
		//@include flex(1 1 0%);
	}

	.cart__image-wrapper {
		//@include flex(0 0 85px);
	}

	.cart__price-wrapper {
		//@include flex(0 1 24%);
		@include rtl-text-align-right();
	}

	.cart__header {
		//@include visually-hidden();
	}

	.cart-message {
		padding-top: 20px;
	}

	.cart__qty {
		padding: 0;
	}

	.cart__qty-label {
		//@include visually-shown();
		@include display(inline-block);
		vertical-align: middle;
		font-size: 13px;
		@include rtl-margin-right(5px);
	}
}

.cart__continue-btn {
	.cart--no-cookies & {
		@include display(none);
	}
}

.cart--empty-message {
	.cart--no-cookies & {
		@include display(none);
	}
}

.cookie-message {
	@include display(none);
	padding-bottom: 25px;

	.cart--no-cookies & {
		@include display(block);
	}
}

.additional-checkout-buttons {
	margin-top: 10px;

	// reset for paypal button
	input[type="image"] {
		padding: 0;
		border: 0;
		background: transparent;
	}
}

