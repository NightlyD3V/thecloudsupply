/* Product Item ===========================================================================================================*/

.item-product {
	position: relative;
	@include transition(all 0.3s linear);
	.thumbnail-container {
		position: relative;
		overflow: hidden;
		@include display(block);
		img {
			margin: 0px auto;
			padding-top: 20px;
		}
		.product__thumbnail-second {
			position: absolute;
			top: 0;
			@include rtl-left(50%);
			bottom: 0;
			@include opacity(0);
			@include transition(all 0.35s ease);
			@include translateX(-100%);
			padding-top: 20px;
		}
	}
	.product__info {
		.product__price {
			color: $color-heading;
			font-size: 1.4rem;
			font-family: $font-family-primary;
			font-weight: bold;
			s.product-price__price {
				color: #c9c9c9;
			    text-decoration: line-through;
    			text-decoration-color: #ff8383;
			}
			.product-price__sale-label {@include display(none);}
			@media (max-width: 991px) {
				font-size: 1.8rem;
			}
		}
		.product__review {	
			.spr-badge-caption {
				@include display(none);
				margin-top: 5px;
				@include rtl-margin-left(10px);
				font-size: 12px;
				color: #909090;
			}
			.spr-starrating {
				@include rtl-margin-right(0);
				.spr-icon {
					@include opacity(1);
				}
				.spr-icon:not(:last-child) {
					@include rtl-margin-right(2px);
				}
				.spr-icon:before {
					@include display(inline-block);
					font: normal normal normal 14px/1 FontAwesome;
					font-size: inherit;
					text-rendering: auto;
					-webkit-font-smoothing: antialiased;
					-moz-osx-font-smoothing: grayscale;
				 	color: #f7bc3d;
				 	font-size: 11px;
				}
				.spr-icon-star:before {
					content: "\f005";
				}
				.spr-icon-star-empty:before {
					content: "\f006";
				}
			}
		}
	}
	.product__title {
		font-weight: lighter;
		display: -webkit-box;
	    -webkit-box-orient: vertical;
	    -webkit-line-clamp: 2;
	    overflow: hidden;
	    padding: 0 20px;
		a {
			color: $theme-color-heading;
			font-size: 13px;
			font-family: $font-family-primary;
			font-weight: 500;
			@media (min-width: 1200px) {
				&:hover {
					color: $theme-color-default;
				}
			}
			@media (max-width: 991px) {
				font-size: 1.8rem;
			}
		}
	}
	.product__description {
		font-family: $font-family-default;
		font-size: 13px;
		color: $theme-color-heading;
		padding-bottom: 40px;
		line-height: 20px;
	}
	.product__available {
		text-transform: uppercase;
		font-size: 1.1rem;
		font-weight: 500;
		i {
			@include rtl-margin-right(10px);
			font-size: 1.4rem;
		}
		&.sold-out {
		}
		&.in-stock {
			color: #6bb853;
		}
	}
	.group-buttons {
		.formAddToCart {
		    @include opacity(1);
			.btnAddToCart {
				background: #6a6a6a;
				position: relative;
				height: 42px;
				width: 180px;
				@include border-radius(40px);
				line-height: 19px;
				i.nov-icon-cart {
					color: $color-bg;
					font-size: 2.4rem;
					position: absolute;
				    top: 10px;
				    @include rtl-left(10px);
				}
				i.zmdi-check {
					color: $color-bg;
					font-size: 2.4rem;
					position: absolute;
				    top: 8px;
				    @include rtl-left(5px);
				}
				.icon-addcart {
			    	background: url({{ 'icon_index_3.png' | asset_url }});
				    width: 15px;
				    height: 17px;
				    @include display(inline-block);
			    	background-position: -16px -18px;
				    background-repeat: no-repeat;
			    }
				span {
					font-size: 11px;
					font-family: $font-family-primary;
					text-transform: uppercase;
					color: $color-bg;
					@include rtl-margin-left(35px);
				}
				@media (min-width: 1200px) {
					&:hover {
						background: $theme-color-default;
						color: $color-bg !important;
					}
				}
			}
		}
		.btn-group-act {
		    @include square(42px);
		    background: #e9e9e9;
		    cursor: pointer;
		    @include flexbox();
		    @include align-items(center);
		    @include justify-content(center);
		    @include border-radius(50%);
		    @include transition(all 0.35s ease);
			i {
				font-size: 3rem;
			    color: #6a6a6a;
			}
			@media (min-width: 1200px) {
				&:hover {
					background: $theme-color-default;
					@include rotate(90deg);
					i {
						color: $color-bg;
					}
				}
			}
		}
		.productWishList {
		    @include opacity(0);
		    visibility: hidden;
		}
		.productQuickView {
		    @include opacity(0);
		    visibility: hidden;
		}
		&.active {
			.formAddToCart {
			    @include opacity(0);
			    visibility: hidden;
			    @include transition(all 0.35s ease);
			}
			.btn-group-act {
			    @include square(42px);
			    background: $theme-color-default;
			    cursor: pointer;
			    @include flexbox();
			    @include align-items(center);
			    @include justify-content(center);
			    @include border-radius(50%);
			    @include transition(all 0.35s ease);
	    		@include rotate(90deg);
	    		i {
	    			color: $color-bg;
	    		}
			}
			.product-button {
				&.active {
					.productWishList {
					    @include opacity(1);
					    @include transition(all 0.35s ease);
					    visibility: inherit;

					}
					.productQuickView {
					    @include opacity(1);
					    @include transition(all 0.35s ease);
					    visibility: inherit;
					}
				}
			}
		}
	}
	.productWishList, .productQuickView {
		.btn {
			background: #e9e9e9;
			padding: 0;
			@include square(42px);
			@include border-radius(50%);
			line-height: 42px;
			font-size: 14px;
			color: #636363 !important;
			@media (min-width: 1200px) {
				&:hover {
					background: $theme-color-default;
					color: $color-bg !important;
				}
			}
		}
	}
	.productWishList .btnProductWishlist.swym-button {
		&:after {
			content: "\f15f" !important;
			font-family: "Material-Design-Iconic-Font";
			font-size: 1.6rem;
			color: #636363 !important;
		}
		@media (min-width: 1200px) {
			&:hover {
				&:after {
					color: $color-bg !important;
				}
			}
		}
		.swym-add-to-wishlist-view-product {
			@include display(block !important);
		}
		&.swym-added {
			@include opacity(1);
			pointer-events: none;
			background: $theme-color-default;
			&:after {
				color: $color-bg !important;
			}
		}
	}
	@media (min-width: 1200px) {
		&:hover {
			.thumbnail-container {
				&.has-multiimage {
					img:not(.product__thumbnail-second) {
						@include opacity(0);
					}
					.product__thumbnail-second {
						@include opacity(1);
						@include translateX(-50%);
					}
				}
			}
		}
	}
	@media (max-width: 1199px) and (min-width: 992px) {
		.productWishList, .productQuickView {
			.btn {
				@include square(35px);
				line-height: 36px;
				&:hover {
					background: $theme-color-default;
					color: $color-bg !important;
				}
			}
		}
	}
	@media (max-width: 767px) {
		.productWishList, .productQuickView {
			.btn {
				@include square(35px);
				line-height: 36px;
			}
		}
	}
	@media (max-width: 524px) {
		.productWishList, .productQuickView {
			.btn {
				@include square(42px);
			}
		}
		.productWishList .btnProductWishlist.swym-button:after {
			font-size: 2rem;
			line-height: 45px;
		}
		.productQuickView i {
			font-size: 2rem;
			line-height: 40px;
		}
	}
}

