//=====================================================================
//Animate Nov slide
@keyframes nov-slide {
	0% {
			@include scale(1,1);
	}
	50% {
			@include opacity(0.3);
			@include scale(2,2);
	}
	100% {
			@include scale(1,1);
	}
}
@-webkit-keyframes nov-slide {
	0% {
			@include scale(1,1);
	}
	50% {
			@include opacity(0.3);
			@include scale(2,2);
	}
	100% {
			@include scale(1,1);
	}
}