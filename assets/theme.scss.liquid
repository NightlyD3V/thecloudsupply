// Vertical menu
.vertical_menu {
	position: absolute;
	top: 100%;
	background: $color-bg;
	min-width: 270px;
	z-index: 9;
	cursor: pointer;
	@include box-shadow(0 0 10px rgba(0,0,0,0.15));
	.site-nav > .site-nav--has-dropdown {
		>a {
			position: relative;
			&:before {
		    content: "";
		    position: absolute;
		    top: 2px;
		    width: 0;
		    height: 51px;
		    background: url({{ 'bf_dropdown_vertical.png' | asset_url }});
		    @include rtl-left(calc(100% + 20px));
		    @include opacity(0);
		    @include transition(all 0.3s linear);
			}
		}
		&:hover {
			a:before {
				@include opacity(1);
				width: 16px;
			}
		}
	}
	.sub_title_nav {
		font-size: 11px;
		color: #bfbdbd;
		font-weight: normal;
		margin-top: 3px;
	}
  	.site-nav {
	  	>li {
		  	@include display(block);
		  	position: static;
		  	list-style-type: none;
		  	&.hide {
				@include display(none);
			}
		  	&:not(:last-child) {
		  		border-bottom: 1px solid #f0f0f0;
		  	}
		  	>a {
		  		.icon_nav {
		  			@include rtl-margin(20px, 5px, 0, 2px);
		  			min-width: 50px;
		  		}
		  		.group_title {
			  		font-family: $font-family-primary;
			  		font-weight: bold;
			  		color: $color-heading;
			  		@include display(block);
			  		text-transform: none;
			  		font-size: 14px;
			  		@include rtl-padding(16.4px, 0, 18px, 0);
		  		}
		  		.sub_title_nav {
					font-size: 11px;
					color: #909090;
					font-family: $font-family-primary;
					font-weight: 500;
					margin-top: 3px;
				}
		  		@include align-items(center);
		  		@media (min-width: 768px) {
			  		&:hover {
			  			background: #f0f0f0;
			  			color: $color-bg !important;
			  		}
		  		}
		  		@media (max-width: 1199px) {
		  			min-height: 37px;
		  		}
		  		@media (max-width: 991px) {
		  		}
		  		@media (max-width: 767px) {
			  		@include rtl-padding-left(40px);
		  		}
		  	}
		  	.megaMenu, .site-nav__dropdown {
		  		@include rtl-left(100% !important);
		  		top: 0;
		  		z-index: 99;
		  		@include rtl-margin-left(5px);
		  		-webkit-transform: translateX(0) !important;
		  		   -moz-transform: translateX(0) !important;
		  		    -ms-transform: translateX(0) !important;
		  		     -o-transform: translateX(0) !important;
		  		        transform: translateX(0) !important;
		  	}
		  	.site-nav__dropdown:not(.megaMenu) {
		  		padding: 5px 20px 5px;
		  		a {
		  			&:hover {
		  				background-color: transparent;
		  				color: $theme_color_default !important;
		  			}
		  		}
		  	}
		  	.site-nav__dropdown .menuTitle:last-child {
				border-bottom: none;
			}
	  	}
	}
	.show_more {
		cursor: pointer;
		@include rtl-padding-left(40px);
		@media (min-width: 1200px) {
			line-height: 5rem;
		}
		i {
			font-size: 2.2rem;
			color: $theme-color-heading;
			@include rtl-margin-right(17px);
			vertical-align: middle;
		}
		span {
			color: #5b5b5b;
			font-weight: bold;
			font-size: 1.4rem;
			font-family: $font-family-default;
		}
		&.active, &:hover {
			span, i {
				color: $theme-color-default;
			}
		}
		&.active {
			i {
				@include rotate(180deg);
			}
		}
		@media (max-width: 1199px) {
			line-height: 4.3rem;
			i {
				font-size: 1.7rem;
			}
			span {
				font-size: 1.3rem;
			}
		}
	}

	@media(max-width: 1199px) { 
		.site-nav>.site-nav--has-dropdown>a:before {
			@include rtl-left(calc(100% + 10px));
		}
	  	.icon_nav {
	  		max-width: 25px;
	  	}
	  	.sub_title_nav {
	  		font-size: 10px;
	  	}
	  	.site-nav--has-dropdown {
	  		&.hasMegaMenu .megaMenu .novMenuLinks li:not(.menuTitle) a, .site-nav__dropdown .site-nav__link {
	  			font-size: 13px;
	  		}
	  		&.hasMegaMenu .megaMenu .novMenuLinks .menuTitle a {
	  			font-size: 13px;
	  		}
	  	}
  	}
	@media(max-width: 991px) and (min-width: 768px) {
	  	&:not(#_mobile_vertical_menu) {
		  	position: fixed;
		  	height: 100vh;
		  	overflow: auto;
		  	background: $color-bg;
	  		text-align: center;
		  	top: 0;
		  	@include rtl-left(-270px);
		  	z-index: 999;
		  	padding: 30px 0px 70px;
		  	width: 270px;
		  	@include border-radius(0);
		  	border: none;
		  	@include transition(all 0.4s linear);
		  	margin-top: 0;
		  	.site-nav {
			  	padding: 0;
		  	}
		  	li {
			    .site-nav__dropdown {
		      		position: static;
		      		width: 100% !important;
		      		@include opacity(1);
		      		visibility: visible;
		      		@include border-radius(0);
		      		@include box-shadow(none);
		      		padding: 0;
		      		background: none !important;
		      		@include display(none);
		      		@include transition(none);
			    }
		  	}
		  	>li>a {
			  	padding: 0 !important;
		  	}
		  	.show_sub i {
			    font-size: 1.9rem;
		  	}
		  	.site-nav>li .megaMenu, .site-nav>li .site-nav__dropdown {
			  	@include rtl-margin-left(-5px);
		  	}
		  	&.active {
			    @include rtl-left(0);
			    @include opacity(1);
		  	}
		}
		.site-nav>.site-nav--has-dropdown>a:before {
			@include display(none);
		}
  	}
  	@media (max-width: 991px) {
  		.site-nav {
  			&>li {
  				&>a {
  					@include align-items(center !important);
  					.group_title {
		  				padding: 10px 0;
		  			}
		  			.icon_nav {
						margin: 0px 5px 0 2px;
		  			}
		  		}
	  		}
  		}
  		&:not(#_mobile_vertical_menu) .show_sub i {
  			font-size: 2.5rem;
  			@include rtl-padding-right(15px);
  		}
  		.site-nav--has-dropdown {
  			&.hasMegaMenu .megaMenu .novMenuLinks li:not(.menuTitle) a, .site-nav__dropdown .site-nav__link {
  				font-size: 13px;
  			}
  		}
  	}
	@media(max-width: 767px) {
	  	.site-nav {
	  		&>.site-nav--has-dropdown>a:before {
		  		@include display(none);
		  	}
		  	&>li>a .group_title {
		  		padding: 9.8px 0 11px 0;
		  	}
	  	}
	  	&.active &#_mobile_vertical_menu {
			padding: 10px 0px 40px;
			@include display(none);
			&.vertical_menu .site-nav>li>a .show_sub {
				margin-top: 25px;
		    	@include rtl-margin-right(10px);
		    	i {
					font-size: 21px;
				}
			}
	  	}
	  	&.active &#_mobile_vertical_menu {
			@include display(block);
	  	}
  	}
}
