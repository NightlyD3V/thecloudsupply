/*=============== Lazy loading ===================*/
.process-loading {
	height: 0;
	@include display(none);
	&:before {
		content: "";
		background: rgba($color-bg,0.6);
		top: 70px;
		@include rtl-left(0);
		@include rtl-right(0);
		bottom: 0;
		position: absolute;
		z-index: 1;
	}
	.loader {
		position: absolute;
		@include rtl-left(50%);
		@include translateX(-50%);
		z-index: 2;
		.dot {
			@include square(20px);
			background: $theme-color-default;
			@include border-radius(100%);
			@include display(inline-block);
			animation: nov-slide 1s infinite;
			&:nth-child(1) {
					animation-delay: 0.1s;
					background: $theme-color-default;
			}
			&:nth-child(2) {
					animation-delay: 0.2s;
					background: #2785af;
			}
			&:nth-child(3) {
					animation-delay: 0.3s;
					background: #7875ac;
			}
			&:nth-child(4) {
					animation-delay: 0.4s;
					background: #bf66aa;
			}
			&:nth-child(5) {
					animation-delay: 0.5s;
					background: $theme-color-primary;
			}
		}
	}
	&.active {
		height: 60px;
		@include display(block);
	}
}

