		@include rtl-margin-right(4px);
	}
}

.share-title {
	@include display(inline-block);
	vertical-align: middle;
}

/* Search Bar ==============================================================================================================================*/
.search-toggle {
	cursor: pointer;
}
.overlay{
	position: fixed;
	@include square(100%);
	top: 0;
	@include rtl-left(0);
	z-index: 10000;
	@include opacity(0);
	visibility: hidden;
	@include transition(opacity 0 0.5s,visibility 0 0.5s);
	&.style-dark-bg {
		background-color: rgba(#000, 0.6) !important;
	}
	&.style-light-bg {
		background-color: $color-bg !important;
	}

	&.overlay-search {
			@include display(table);
			table-layout: fixed;
			@include square(100%);
			text-align: center;
			.search-container form{
				@include opacity(1);
				@include translate(-50%,-50%);
				-webkit-transition: -webkit-transform 0.2s,opacity 0.2s;
				transition: transform 0.2s,opacity 0.2s;
				@include display(inline-block);
				width: 100%;
				max-width: 600px;
				max-width: 60vw;
				position: absolute;
				top: 50%;
				@include rtl-left(50%);
				input {
					@include display(inline-block);
					margin: 0px;
					width: 100%;
					font-size: 2.8rem;
					font-size: 3vh;
					@include rtl-padding(.8em, 3em, .8em ,1.5em);
					border: none;
					border-bottom: 1px solid #d0d0d0;
					color: $theme-color-heading;
					background-color: transparent;
					@include border-radius($border-radius);
					outline: none;
					@include placeholder {
						color: #cfcfcf;
					}
				}
				.search__des {
					color: #343434;
					@include opacity(0);
					@include translateY(-40px);
					@include transition(transform 0.5s,opacity 0.5s);
					-webkit-transition-delay: 0.1s;
					-moz-transition-delay: 0.1s;
					-ms-transition-delay: 0.1s;
					-o-transition-delay: 0.1s;
					transition-delay: 0.1s;
					i {
						font-size: 7.2rem;
					}
					h2 {
						font-size: 3.6rem;
						margin-top: 25px;
						margin-bottom: 20px;
					}
					div {
						font-size: 11px;
						font-weight: 500;
						text-transform: uppercase;
						width: 100%;
						max-width: 440px;
						margin: 0 auto;
						margin-bottom: 0px;
						margin-bottom: 85px;
					}
				}
				.search-container-inner {
					position: relative;
					@include opacity(0);
					@include translateY(-40px);
					@include transition(transform 0.3s,opacity 0.3s);
					-webkit-transition-delay: 0.5s;
					-moz-transition-delay: 0.5s;
					-ms-transition-delay: 0.5s;
					-o-transition-delay: 0.5s;
					transition-delay: 0.5s;
				}
			}
			.close-search {
				color: #343434;
				position: absolute;
				top: 50px;
				@include rtl-right(50px);
				font-size: 3.6rem;
				cursor: pointer;
			}
			.search-bar__submit {
				font-size: 2.8rem;
				color: #dfdfdf;
				position: absolute;
				top: 50%;
				@include rtl-right(0.8em);
				@include translateY(-50%);
			}
	}
	&.open {
			@include opacity(1);
			visibility: visible;
			@include transition(opacity 0.5s);
			.search-container form {
				.search__des, .search-container-inner {
					@include opacity(1);
					@include translateY(0);
				}
			}
	}
}

/*============================================================================
	The search submit button has pointer-events: none which also
	effects the :hover style. This forces the style to be applied.
==============================================================================*/
.search-header__input:hover + .btn--link {
	opacity: $opacity-link-hover;
}

/*================ Mobile Search Bar ================*/

.search-bar {
	border-bottom: 1px solid $color-border;
	padding: 0 ($gutter-site / 2);
}

.search-bar__table {
	@include display(table);
	table-layout: fixed;
	@include square(100%);
}

.search-bar__table-cell {
	@include display(table-cell);
	vertical-align: middle;
}

.search-bar__form-wrapper {
	width: 90%;
}

/*================ Header Search ================*/
.site-header__search {
	position: relative;
	.site-header__search-icon i{
		font-size: 1.9rem;
	}
}
.search-header {
	@include display(none);
	position: absolute;
	width: 100%;
	max-width: 30px;
	vertical-align: middle;

	&.search--focus {
		max-width: 250px;
	}
}


.search--focus {
	.search-header__input {
		outline: none;
		border-color: $color-border;
	}

	.search-header__submit {
		@include rtl-padding-left(10px);
	}
}

.search-header__submit {
	cursor: pointer;
	&:focus, &:hover {
		outline: none;
	}
}

.search-header,
.search-header__submit {
	transition: all 0.35s cubic-bezier(0.29, 0.63, 0.44, 1);
}


$return-button-width: 55px;
$nav-button-padding: 15px;

/* Mobile Site Nav =============================================================================================================*/
.mobile-nav {
	@include display(block);
	@include transform(translate3d(0, 0, 0));
	transition: $transition-drawer;

	.sub-nav--is-open & {
		@include transform(translate3d(-100%, 0, 0));
	}

	.third-nav--is-open & {
		@include transform(translate3d(-200%, 0, 0));
	}
}

.mobile-nav__link,
.mobile-nav__sublist-link {
	@include display(block);
	width: 100%;
	padding: $nav-button-padding ($nav-button-padding * 2);
	font-size: $font-size-mobile-input;
}

.mobile-nav__link {
	position: relative;
}

.mobile-nav__sublist-link:not(.mobile-nav__sublist-header) {
	padding-left: $return-button-width + $nav-button-padding;
	padding-right: ($nav-button-padding * 2);
}

.mobile-nav__item {
	@include display(block);
	width: 100%;

	.icon {
		position: absolute;
		top: 50%;
		@include rtl-left(50%);
		height: 12px;
		width: 10px;
		@include rtl-margin(-6px, 0, 0, -5px);
	}
}

.mobile-nav__return {
	@include rtl-border-right(1px solid $color-border);
}

.mobile-nav__return-btn {
	position: relative;
	padding: 24px 0;
	width: $return-button-width;
}

.mobile-nav__icon {
	position: absolute;
	@include rtl-right(0);
	top: 0;
	bottom: 0;
	padding-left: $gutter-site-mobile;
	padding-right: $gutter-site-mobile;
	pointer-events: none;
	overflow: hidden;
}

.mobile-nav__table {
	@include display(table);
	width: 100%;
}

.mobile-nav__table-cell {
	@include display(table-cell);
	vertical-align: middle;
	width: 1%;
	@include rtl-text-align-left();
	white-space: normal;
}

.mobile-nav__toggle-button {
	padding: 20px 15px;
}

.mobile-nav__dropdown {
	position: absolute;
	background-color: $color-body;
	z-index: $z-index-sub-nav;
	width: 100%;
	top: 0;
	@include rtl-right(-100%);
	@include display(none);

	.is-active + & {
		@include display(block);
		@include opacity(1);
	}

	// Need the transition so `prepareTransition` removes class
	&.is-closing {
		transition: $transition-drawer;
		@include opacity(0.99);
	}

	.mobile-nav__sublist-header {
		font-family: $font-stack-header;
		font-weight: $font-weight-header;
		@include display(table-cell);
		vertical-align: middle;
		padding-left: $nav-button-padding;
	}
}

/* Mobile nav wrapper =======================================================================================*/
.mobile-nav-wrapper {
	@include transform(translate3d(0, -100%, 0));
	position: absolute;
	background-color: $color-body;
	transition: $transition-drawer;
	@include display(none);
	overflow: hidden;
	width: 100%;

	&::after {
		content: '';
		position: absolute;
		bottom: 0;
		@include rtl-left(0);
		@include rtl-right(0);
		border-bottom: 1px solid $color-border;
	}

	&.js-menu--is-open {
		@include display(block);
	}
}

.mobile-nav--open {
	.icon-close {
		@include display(none);
	}
}

.mobile-nav--close {
	.icon-hamburger {
		@include display(none);
	}
}

/* Modals ============================================================================================*/
.modal-dialog-centered {
	@include flexbox();
	@include align-items(center);
	min-height: calc(100% - (2.5rem * 2));
}
.modal-backdrop {
	background: rgba(0,0,0,0.5);
}
.modal {
	.modal-header {
		padding: 0;
		border-bottom: none;
		position: relative;
		z-index: 9;
		button {
			@include square(22px);
			background: $theme-color-default;
			@include border-radius(50%);
			position: absolute;
			@include rtl-right(0);
			top: 0px;
			line-height: 2.2rem;
			z-index: 99;
			padding: 0;
			i {
				color: $color-bg;
				font-size: 1.4rem;
				width: 22px;
			}
			&:hover {
				outline: none;
			}
		}
		.close {
			@include opacity(1);
		}
	}
}
.modal-open {
	padding: 0 !important;
	overflow: inherit;
	.modal {
		overflow-y: hidden;
	}
}

// Nov Slick ====================================================================================
.nv-slick-slider {
	.arrow-prev {
		font-size: 3rem;
		position: absolute;
		cursor: pointer;
		color: #a1a1a1;
		text-align: center;
		top: -70px;
		z-index: 9;
		.zmdi-chevron-right {
			@include rtl-right(37px);
		    padding: 2px 13px;
		    @include border-radius(4px);
		    color: $color-bg;
		    margin-top: 7px;
		    font-size: 1.8rem;	
		    background: #b4cbd6;
		}
		&.slick-disabled {
			pointer-events: none;
			@include opacity(0.6);
		}
		&.slick-arrow {
			@include rtl-right(54px);
			&:before {
				content: "";
				position: absolute;
				width: 1px;
				height: 16px;
				background: #e0e0e0;
				top: 14px;
				@include rtl-right(-15px);
			}
		}
		&.arrow-next {
			@include rtl-right(15px);
		}
	}
}
.nov-slick-carousel {
	.slick-current {
		.item-product {
			border-color: transparent !important;
		}
	}
	.slick-slide:not(.slick-active) {
		.item-product {
			border-color: transparent;
			&.type_1:before {
				@include display(none);
			}
		}
	}
	.slick-slide>div:not(:last-child) .item-product {
		margin-bottom: 20px;
	}
	.item {
		padding: 0;
		img {
			margin: auto;
		}
	}
	.slick-arrow {
		font-size: 1.6rem;
		position: absolute;
		cursor: pointer;
		color: $color-bg;
		text-align: center;
    	top: -68px;
		@include border-radius(5px);
    	.zmdi {
    	}
    	&.arrow-prev {
    	}
    	&.arrow-next {
			@include rtl-right(0);
    	}
		&.slick-disabled {
			pointer-events: none;
			@include opacity(0.6);
		}
	}
	&.nav_top {
		.slick-arrow {
			top: -68px;     
			&.arrow-prev, &.arrow-next {
			    color: #9b9b9b;
			    background: $color-bg;
			    font-size: 1.8rem;
			}
			@media (min-width: 1200px) {
				&:hover {
					background: $theme-color-default;
				}
			}
		}
	}
	&.nav_center {
		.slick-arrow {
			top: 50%;
			@include translateY(-50%);
			@include square(40px);
			@include border-radius(50% !important);
		}
	}
	&.nav_bottom {
		.slick-arrow {
			bottom: 0px;
			&.arrow-prev {
				@include rtl-left(calc(50% - 51px));
			}
			&.arrow-next {
				@include rtl-right(calc(50% - 50px));
			}
		}
	}
