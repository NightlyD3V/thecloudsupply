}
.ratio-container:after {
		content:'';
		@include display(block);
		height: 0;
		width: 100%;
		/* 16:9 = 56.25% = calc(9 / 16 * 100%) */
		padding-bottom: 50%;
		content:"";
}
.ratio-container > * {
		position: absolute;
		top: 0;
		@include rtl-left(0);
		@include square(100%);
}

