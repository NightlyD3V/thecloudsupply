}

/*================ Button variations ================*/
.btn--link {
	background-color: transparent;
	border: 0;
	margin: 0;
	color: $color-body-text;
	@include rtl-text-align-left();

	&:hover,
	&:focus {
		opacity: $opacity-link-hover;
	}

	.icon {
		vertical-align: middle;
	}
}

.btn--narrow {
	@include rtl-padding-left(15px);
	@include rtl-padding-right(15px);
}

.btn--disabled {
	@include opacity(0.5);
	cursor: default;

	&:hover,
	&:focus {
		@include opacity(0.5);
	}
}

.btn--has-icon-after {
	.icon {
		@include rtl-margin-left(10px);
	}
}

.btn--has-icon-before {
	.icon {
		@include rtl-margin-right(10px);
	}
}

/*================ #Images and Iframes ================*/
svg:not(:root) {
	overflow: hidden;
}

.video-wrapper {
	position: relative;
	overflow: hidden;
	max-width: 100%;
	padding-bottom: 56.25%;
	height: 0;
	height: auto;

	iframe {
		position: absolute;
		top: 0;
		@include rtl-left(0);
		@include square(100%);
	}
}

/* Title Block ===================================================== */
.title_block {
	font-size: 3rem;
	color: $theme-color-default;
	position: relative;
	font-family: $font-family-primary;
	font-weight: bold;
	text-transform: uppercase;
	@include border-radius(5px 0 0 0);
	span {
		position: relative;
	}
	.sub_title {
		font-size: 11px;
	    font-family: $theme-color-default;
	    color: $theme-color-heading;
	    font-weight: normal;
	    @include display(block);
		&.d-inline {
			font-family: $font-family-default;
		    vertical-align: middle;
			@media(max-width: 991px) {
				@include display(block !important);
				@include rtl-margin-left(0 !important);
			}
			@media(max-width: 320px) {
				@include display(block !important);
				@include rtl-margin-left(0 !important);
			    position: absolute;
			    top: 100%;
			}
		}
	}
}

/*================ #Site Nav and Dropdowns ================*/
.site-nav {
	position: relative;
	padding: 0;
	@include rtl-text-align-left();
	>li>a {
		font-family: $font-family-primary;
		font-size: 12px;
		color: $mainmenu-link-color;
		font-weight: 500;
		text-transform: uppercase;
	}
	a {
		color: #848484;
		font-family: $font-family-default;
	    @include display(block);
	    text-transform: none;
	    font-size: 12px;
		&:hover {
			text-decoration: none;
			color: $theme-color-default !important;
		}
	}
	li {
		@include display(inline-block);
		&.site-nav--active {
			>a {
				color: $theme-color-default !important;
			}
		}
	}
}

/*================ Site Nav Links ================*/
.site-nav__link {
	@include display(block);
	white-space: nowrap;
	.site-nav--centered & {
		padding-top: 0;
	}

	.zmdi-chevron-down {
		@include square(8px);
		@include rtl-margin-left(2px);
	}

	&.site-nav--active-dropdown {
		border: 1px solid $color-border;
		border-bottom: 1px solid transparent;
		z-index: 2;
	}
}
.site-nav--active-dropdown {
	.zmdi-chevron-down {
		&:before {
			content: '\f2fc';
		}
	}
}

/*================ Dropdowns ================*/

.site-nav__dropdown {
	@include opacity(0);
	visibility: hidden;
	position: absolute;
	margin: 0;
	z-index: $z-index-dropdown;
	@include rtl-text-align-left();
	background: $color-bg;
	@include rtl-left(-1px);
	top: 130%;
	@include box-shadow(0 0 10px rgba(#000, 0.15));
	min-width: 200px;
	@include transition(all 0.2s linear);
	@media (max-width: 767px){
		width: 100% !important;
		.novMenuLinks {
			li {
				@include rtl-margin-left(20px);
				a {
					color: $theme-color-heading;
					font-size: 13px;
					font-family: $font-family-primary;
				}
				&.menuTitle {
					@include rtl-margin-left(0px);
					a {
						color: $theme-color-second;
						font-weight: bold;
						font-size: 13px;
						font-family: $font-family-primary;
					}
				}
			}
		}
	}
	@media (min-width: 768px) and (max-width: 1024px) {
		max-width: 98vw;
	}
	.site-nav__link {
		padding: 10px 20px;
		@include transition(all 0.1s linear);
		&:hover {
			background: $theme-color-default;
			color: $color-bg !important;
		}
	}

	.site-nav--active-dropdown & {
		@include opacity(1);
		visibility: visible;
		top: 100%;
	}
	ul {
		@include rtl-padding-left(0);
	}
	li {
		border-bottom: 1px solid #e5e5e5;
		@include display(block);
		&:last-child {
			border-bottom: none;
		}
	}
}
//Megamenu
.site-nav--has-dropdown {
	position: relative;
	&:hover {
		.site-nav__dropdown {
			@include opacity(1);
			visibility: visible;
			top: 100%;
			z-index: 999;
		}
	}
	&.hasMegaMenu {
		@include display(inline-flex);
		&.center {
			position: static;
			&:before {
				@include display(none);
			}
			.megaMenu {
				@include rtl-left(0%);
			}
			@media(max-width: 767px) {
				position: relative;
			}
		}
		.megaMenu {
			@include rtl-padding(25px, 35px, 36px, 25px);
			.novMenuLinks {
				a {
					padding: 10.35px 0;
					@include display(block);
					@include transition(all 0.2s linear);
					font-size: 13px;
					span {
						font-size: 12px;
					}
				}
				.menuTitle {
					@include display(block);
					border-bottom: none;
					a {
						font-family: $font-family-primary;
						color: $color-heading;
						text-transform: uppercase;
						font-size: 1.4rem;
						font-weight: 900;
					}
					&:not(.has_child) {
						font-weight: normal;
						font-size: 13px;
						text-transform: none;
						border-bottom: 1px solid #e5e5e5;
					}
					&.has_child {
						a {
							padding: 10px 0 20px 0;
						}
					}
				}
				li:not(.menuTitle) a:hover {
					color: $theme-color-primary;
				}
			}
			.novMenuBanner {
				margin-top: 15px;
				&.has_shadow {
					box-shadow: 0 0 15px rgba(#000, 0.2);
				}
				a {
					padding: 0;
				}
			}
			@media (max-width: 1199px) {
				max-width: 700px;
			}
		}
	  	&:first-child {
	  		.site-nav__dropdown {
	  			@include rtl-padding(18px, 26px, 0, 26px);
			    .novMenuLinks {
			      	.menuTitle a {
				        font-family: $font-family-primary;
				        font-size: 13px;
				        color: #848484;
				        font-weight: 500;
				        text-transform: none;
			      	}
			    }
	  		}
	  	}
	  	&:nth-child(2) {
	  		.site-nav__dropdown {
			    .novMenuLinks {
			    	padding-bottom: 20px;
			    }
	  		}
	  		.novMenuBanner {
	  			margin-bottom: 14px;
	  		}
	  	}
	  	&:nth-child(3), &:nth-child(4) {
	  		.site-nav__dropdown {
			    .novMenuLinks {
			      	li {
			      		a {}
			      	}
			    }
	  		}
	  		.megaMenu {
	  			@include rtl-padding(25px, 35px, 85px, 25px);
	  		}
	  	}
	  	@media (min-width: 768px) {
	  		&:nth-child(3), &:nth-child(4) {
		  		.site-nav__dropdown {
				    .novMenuLinks {
				      	li {
				      		border-bottom: none;
				      		a {
						        padding: 5.35px 0;
						    }
					    }
			      	}
			    }
	  		}
	  	}
	}
	@media (max-width: 991px) {
		&.hasMegaMenu {
			.megaMenu .novMenuLinks .menuTitle.has_child a {
				padding: 10px 0;
			}
			&:nth-child(2), &:nth-child(3) {
				.site-nav__dropdown .novMenuLinks {
					padding-bottom: 0;
				}
				.novMenuBanner {
			        margin-bottom: 0px;
				    padding: 0 10px;
				    @include rtl-padding-left(20px);
				}
			}
			&.center {
				.megaMenu {
					@include rtl-left(-15%);
				}
			}
		}
	}
	@media (max-width: 767px) {
		&.hasMegaMenu {
			.megaMenu .novMenuLinks .menuTitle.has_child a {
				padding: 10px 0;
			}

			&:nth-child(1) {
                .novMenuLinks {
                	@include rtl-padding-left(52px);
                }
			}

			&:nth-child(2), &:nth-child(3), &:nth-child(4) {
				.site-nav__dropdown .novMenuLinks {
					padding-bottom: 0;
					@include rtl-padding-left(50px);
				}
				.novMenuBanner {
			        @include display(none);
				}
			}
		}
	}
}

