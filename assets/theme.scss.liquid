				li.list-inline-item {
					@include square(35px);
					line-height: 25px;
					text-align: center;
					border: 1px solid #dedede;
					cursor: pointer;
					margin-bottom: 10px;
					&:hover, &.active {
						background: $theme-color-default;
					  	border-color: transparent;
					  	span {
							color: $color-bg !important;
					  	}
					}
					.filter-option-value {
					  	padding-top: 4px;
					  	@include display(inline-block);
					}
				}
			}
		}
	}
}

/* sortPagiBar =============================================================================================================================*/
.sortPagiBar {
	background: $color-bg;
	position: relative;
	margin: 0 0 30px 0;
	> div {
		&:not(:last-child) {
			@include rtl-padding-right(10px);
		}
	}
	&:before {
		content: "";
		width: 100%;
		height: 1px;
		background: #e7e7e7;
		position: absolute;
		top: 50%;
		@include rtl-left(0);
	}
	.gridlist-toggle {
		background: $color-bg;
		z-index: 1;
		a {
			@include display(inline-block);
			background: #c4c4c4;
			color: $color-bg;
			font-size: 1.4rem;
			text-align: center;
			@include border-radius(3px);
			&.active,&:hover {
				background: $theme-color-default;
				color: $color-bg !important;
			}
			&:not(:last-child) {
				@include rtl-margin-right(2px);
			}
		}
		i {
			font-size: 14px;
		    @include square(35px);
		    line-height: 35px;
		}
	}
	.showing-total {
		font-size: 11px;
		text-transform: uppercase;
		color: #909090;
		background: $color-bg;
		z-index: 1;
	}
	.filters-toolbar__item {
		@include rtl-padding-left(10px);
		z-index: 1;
		background: $color-bg;
		position: relative;
		.btn {
			font-size: 11px;
			padding: 6px 10px;
			height: 35px;
			border: 1px solid #dfdfdf;
			color: #909090;
			font-weight: 400;
			background: $color-bg;
			font-family: $font-family-default;
			@media (min-width: 1200px) {
				&:hover {
					color: $theme-color-default !important;
				}
			}
		}
		.dropdown-toggle {
			&::after {
				@include rtl-margin-left(42px !important);
			}
		}
		.dropdown-menu {
			min-width: 200px;
			padding: 5px 0;
			.dropdown-item {
				font-size: 1.4rem;
				color: #909090;
				&:hover,&.active {
					cursor: pointer;
					color: $theme-color-default;
					background: $color-bg;
				}
			}
		}
	}
}

