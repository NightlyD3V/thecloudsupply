/* Sidebar ==============================================================================================================================*/
.sidebar {
	.title-block {
		color: $color-heading;
		font-weight: bold;
		text-transform: uppercase;
		font-size: 1.8rem;
		font-family: $font-family-primary;
		margin-bottom: 23px;
	}
	.sidebar-block {
		margin-bottom: 36px;
		.item-product {
			.thumbnail-container {
				border-right: none;
				img {
					padding-top: 0;
				}
			}
			.product__title {
				padding: 0;
			}
		}
	}
	.product__review {
		.spr-badge-caption {@include display(none);}
	}
}
.sidebar__list {
	list-style: none;
	margin-bottom: $gutter-site;
	li {
		margin-bottom: 10px;
	}
}
.sidebar-collection {
	.categories__sidebar {
		.block__content {
			> .cateTitle {
				padding: 10px 0;
				&:first-child {
					padding-top: 0;
				}
				&:not(:last-child) {
					border-bottom: 1px dotted #e4e9eb;
				}
				>a {
					font-size: 1.4rem;
					color: $theme-color-second;
					font-family: $font-family-primary;
				    font-weight: 600;
					&:hover {
						color: $theme-color-default;
						text-decoration: none;
					}
					@media(max-width: 991px) and (min-width: 768px) {
						font-size: 11px;
					}
				}
			}
			.hasSubCategory {
				position: relative;
				.arrow {
					position: absolute;
					@include rtl-right(0);
					top: 11px;
					&:after {
						content: "\f273";
						background: $theme-color-default;
						font-family: 'Material-Design-Iconic-Font';
						@include square(15px);
						padding: 1px 4px;
						color: $color-bg;
						@include border-radius(2px);
					}
					&:hover {
						cursor: pointer;
					}
					&.collapsed {
						&:after {
							content: "\f278";
							background: #cadae2;
							color: $color-bg;
						}
						&:hover {
							cursor: pointer;
							&:after {
								background: $theme-color-default;
							}
						}
					}
				}
				&.level2 {
					.arrow {
						&:after {
							background: none;
							color: #898989;
						}
						&:hover {
							&:after {
								background: none;
								color: $theme-color-default;
							}
						}
					}
				}
				&:first-child {
					.arrow {
						top: 0;
					}
				}
			}
			.subCategory {
				.cateTitle {
					padding: 5px 0;
				}
				.cateItem {
					color: $theme-color-heading;
					font-size: 1.3rem;
					@media (max-width: 1280px) {
						&:hover {
							color: $theme-color-default;
							&:before {
								background: $theme-color-default;
							}
						}
					}
					&:before {
						content: "";
						@include display(inline-block);
						@include square(4px);
						background: #6c6c6c;
						@include rotate(45deg);
						@include rtl-margin-right(22px);
						margin-bottom: 2px;
					}
				}
			}
			.level2 > .subCategory {
				@include rtl-padding-left(35px);
				.cateItem::before {@include display(none);}
			}
		}
	}
	.productlist__sidebar {
		.item-product {
			background: none;
			border: 1px solid #e6e6e6;
			@include border-radius(0);
			&:before {
				@include display(none);
			}
			.thumbnail-container {
				overflow: hidden;
			}
			.product__info {
				padding: 10px;
			}
			&:hover {
			}
			@media(max-width: 1199px) {
				.thumbnail-container { 
					width: 70px;
					min-width: 70px;
				}
				.product__info {
					padding: 10px 0;
					.product__title {
					}
					.product__review .spr-badge-caption {
						@include display(none);
					}
				}
			}
			@media(max-width: 991px) and (min-width: 768px) {
				@include display(block !important);
				text-align: center;
				.thumbnail-container {
					width: 100%;
					@include rtl-border-right(none);
				}
				.product__info {
					@include rtl-margin-left(0 !important);
					margin-top: 10px;
					.product__title {
						a {
							font-size: 13px;
						}
					}
				}
			}
			@media (max-width: 767px) {
				.thumbnail-container {
					width: 100px;
					min-width: 100px;
					padding-top: 15px;
				}
				.product__info {
					padding: 30px 0;
				}
			}
		}
	}
}
