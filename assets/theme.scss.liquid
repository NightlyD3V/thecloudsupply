				@include transition(all 0.3s linear);
				text-decoration: none;
				@include display(inline-block);
				position: relative;
				overflow: hidden;
				&:hover {
					background: $theme-color-default;
					&:after {
						@include rtl-right(-65%);
	    				top: 120%;
