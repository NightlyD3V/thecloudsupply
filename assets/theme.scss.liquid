/* Pagination ==============================================================================================================================*/
.nov-pagination {
	background: $color-bg;
	margin-top: 20px;
	position: relative;
	@include justify-content(center);
	&:before {
		content: "";
		position: absolute;
		width: 100%;
		height: 1px;
		background: #e7e7e7;
		top: 50%;
		@include rtl-left(0);
		z-index: 2;
		@include display(none);
	}
}
.pagination__viewing {
	font-size: 11px;
	color: #909090;
	text-transform: uppercase;
	background: $color-bg;
	z-index: 2;
	@media(max-width: 480px) {
		@include display(none);
	}
}

.pagination {
	background: $color-bg;
	text-align: center;
	list-style: none;
	font-size: 11px;
	z-index: 2;
	@include rtl-padding-left(10px);
	li {
		@include display(inline-block);
		&:not(:last-child) {
			@include rtl-margin-right(5px);
		}
	}
}

.pagination__text {
	@include square(35px);
	line-height: 35px;
	background: #c4c4c4;
	color: $color-bg;
	font-size: 1.4rem;
	@include border-radius(4px);
	a {
		color: $color-bg;
		&:hover {
			color: $color-bg !important;
			text-decoration: none;
		}
	}
	&.active, &:hover {
		background: $theme-color-default;
		border: 1px solid $theme-color-default;
	}
	&.active {
		&:hover {
			cursor: not-allowed;
		}
	}
}

.pagination__btn {
	padding: 9px 18px;
	color: #909090;
	border: 1px solid #dfdfdf;
	font-size: 11px;
	@include border-radius(4px);
	text-transform: uppercase;
	font-family: $font-family-default;
	@media (min-width: 1200px) {
		&:hover {
			text-decoration: none;
			color: $color-bg !important;
			background: $theme-color-default;
			border-color: $theme-color-default;
		}
	}
	&.disabled {
		@media (min-width: 1200px) {
			&:hover {
				cursor: not-allowed;
			}
		}
	}
}

