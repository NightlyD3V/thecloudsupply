// Product thumb
.section-product-thumb {
	.item-product {
		.product__info {
			padding: 0;
			.product__title {
				margin-bottom: 0;
				a {
					font-size: 2.6rem;
					color: $theme-color-default;
					font-family: $font-family-primary;
				}
			}
			.product__review {
				margin-bottom: 10px;
				.spr-badge-caption {
					@include display(inline-block);
					margin-top: 0;
					vertical-align: bottom;
					@include rtl-margin-left(10px);
				}
			}
			.product__price {
				margin-bottom: 15px;
				.product-price__sale {
					font-size: 2.4rem;
				}
				.product-price__price:not(.product-price__sale) {
					font-size: 2rem;
				}
			}
			.product__description {
				margin-bottom: 25px;
			}
		}
		.group-buttons {
			position: absolute;
			@include rtl-left(50px);
			bottom: 35px;
			@include opacity(0);
			@include transition(all 0.3s linear);
		}
		&:hover {
			@include scale(1, 1);
			@include box-shadow(none);
			.product__info {
				.product__review, .product__price {
					@include opacity(1);
					@include translateY(0);
					visibility: visible;
				} 
			}
			.countdownfree {
				@include transition(all 0.3s linear);
				@include opacity(0);
				@include translateY(-30px);
			}
			.group-buttons {
				@include rtl-right(0);
				@include opacity(1);
			}
		}
	}
	.slider-for {
		.item-product {
			border-color: $theme_color_primary;
			@include rtl-padding(0, 0, 0, 55px);
			.thumbnail-container a {
				position: relative;
				@include display(block);
			}
		}
	}
	.slider-nav {
		margin: 30px -15px 0;
		.item-product {
			border-color: $theme-color-default;
		}
		.item {
			padding: 0 15px;
			&:hover, &.slick-current {
				.item-product {
					border-color: $theme-color-primary;
				}
			}
		}
	}
	.slick-arrow {
		position: absolute;
		@include square(40px);
		@include border-radius(50%);
		background: $theme-color-default;
		text-align: center;
		font-size: 1.5rem;
		line-height: 4rem;
		color: $color-bg;
		cursor: pointer;
		top: 50%;
		@include translateY(-50%);
		z-index: 9;
		&.arrow-prev {
			@include rtl-left(-5px);
		}
		&.arrow-next {
			@include rtl-right(-5px);
		}
		&:hover {
			background: $theme-color-primary;
		}
	}
	@media(max-width: 1199px) {
		.item-product .product__info {
			padding: 20px 0 25px 0;
		}
	}
	@media(max-width: 991px) {
		.item-product .product__info .product__title a {
			font-size: 2rem;
		}
		.slider-for {
			.item-product {
				@include rtl-padding-left(25px);
				.countdownfree .item-time {
					&:not(:last-child) {
						@include rtl-margin-right(10px);
					}
					.data-time span:not(.name-time) {
						@include square(45px);
						line-height: 4.5rem;
						background-size: contain;
					}
				}
			}
		}
	}
	@media(max-width: 767px) {
		margin-top: 30px;
		.product-slider {
			padding: 10px 15px;
		}
		.slider-for .item-product {
			padding: 15px !important;
			text-align: center;
		}
	}
}

