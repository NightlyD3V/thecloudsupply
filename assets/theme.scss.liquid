	&.style_1 {
		.slick-dots li {
			button {
				@include square(22px);
				color: $color-bg;
				&:before {
					@include square(22px);
					background: $color-bg;
					@include opacity(0.3);
				}
			}
			&.slick-active, &:hover {
				button {
					color: #45ab67;
					&:before {
						background: $color-bg;
						@include opacity(1);
					}
					
				}
			}
		}
	}
	&.style_2 {
		.slick-dots li {
			margin: 5px;
			button {
				@include square(21px);
				color: transparent;
				&:before {
					@include square(21px);
					background: rgba($color-bg, 0.5);
				}
			}
			&.slick-active, &:hover {
				button:before {
					background: $theme-color-default;
				}
			}
		}
	}
	&.style_3 {
		.slick-dots li {
			button {
				@include square(21px);
				height: 21px;
				color: transparent;
				&:before {
					@include square(21px);
					background: $color-bg;
				}
			}
			&.slick-active, &:hover {
				button:before {
					background: $theme-color-primary;
				}
			}
		}
	}
	@media (max-width: 1199px) {
		.content-caption {
			padding: 0 15px;
		}
		.slick-slide .slide-image {
		    max-width: 100%;
		}
	}
